{
  "name": "hhs-heart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm install --legacy-peer-deps && next build",
    "build:no-lint": "npm install --legacy-peer-deps && next build --no-lint",
    "build:deploy": "npm install --legacy-peer-deps && next build --no-lint --no-typescript",
    "start": "next start",
    "lint": "next lint",
    "optimize-images": "node scripts/optimize-images.js",
    "generate-icons": "node scripts/generate-icons.js",
    "analyze": "cross-env ANALYZE=true next build",
    "setup-analyzer": "node scripts/analyze-bundle.js",
    "analyze-bundle": "node scripts/analyze-bundle.js --run",
    "build:prod": "npm install --legacy-peer-deps && npm run generate-icons && npm run optimize-images && next build",
    "start:prod": "next start"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.4",
    "@react-three/fiber": "^9.1.0",
    "@types/react-scroll": "^1.8.10",
    "@types/three": "^0.174.0",
    "critters": "^0.0.23",
    "netlify": "^13.3.3",
    "next": "13.5.6",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.16.0",
    "react-parallax": "^3.5.1",
    "react-scroll": "^1.9.3",
    "three": "^0.174.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.6",
    "@types/node": "^20.11.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "13.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35"
  },
  "engines": {
    "node": "18.x"
  }
}
