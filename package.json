{
  "name": "hhs-heart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:no-lint": "next build --no-lint",
    "build:deploy": "next build --no-lint --no-typescript",
    "start": "next start",
    "lint": "next lint",
    "optimize-images": "node scripts/optimize-images.js",
    "generate-icons": "node scripts/generate-icons.js",
    "analyze": "cross-env ANALYZE=true next build",
    "setup-analyzer": "node scripts/analyze-bundle.js",
    "analyze-bundle": "node scripts/analyze-bundle.js --run",
    "build:prod": "npm run generate-icons && npm run optimize-images && next build",
    "start:prod": "next start"
  },
  "dependencies": {
    "@react-three/drei": "^10.0.4",
    "@react-three/fiber": "^9.1.0",
    "@types/react-scroll": "^1.8.10",
    "@types/three": "^0.174.0",
    "critters": "^0.0.23",
    "netlify": "^13.3.3",
    "next": "15.2.1",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-dom": "^18.0.0",
    "react-intersection-observer": "^9.16.0",
    "react-parallax": "^3.5.1",
    "react-scroll": "^1.9.3",
    "three": "^0.174.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@next/bundle-analyzer": "^14.2.24",
    "@tailwindcss/postcss": "^4.0.12",
    "@types/node": "^20.17.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.2.1",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.2"
  }
}
